{
  "extends": "@cygns/eslint-config-node",
  "env": {
    "mocha": true
  },
  "rules": {
    "func-names": "off",
    "import/no-extraneous-dependencies": "off",
    "import/no-unresolved": "off",
    "no-console": "off",
    "prefer-arrow-callback": "off",
    "no-unused-expressions": "off"
  }
}
